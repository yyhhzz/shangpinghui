{"ast":null,"code":"import { mapState } from \"vuex\";\nimport throttle from \"lodash/throttle\";\nexport default {\n  name: \"TypeNav\",\n\n  // 组件挂载完毕，向服务器发送请求\n  data() {\n    return {\n      currentIndex: -1\n    };\n  },\n\n  mounted() {\n    this.$store.dispatch(\"categoryList\"); // 派发action\n  },\n\n  computed: { ...mapState({\n      categoryList: state => state.home.categoryList\n    })\n  },\n  methods: {\n    // 三级联动节流 throttle\n    enterHandle: throttle(function (index) {\n      this.currentIndex = index;\n    }, 10),\n\n    leaveHandle() {\n      this.currentIndex = -1;\n    },\n\n    goSearch(event) {\n      let targetNode = event.target;\n      let {\n        categoryname,\n        category1id,\n        category2id,\n        category3id\n      } = targetNode.dataset;\n\n      if (categoryname) {\n        var locations = {\n          name: \"search\",\n          query: {\n            categoryName: categoryname\n          }\n        };\n\n        if (category1id) {\n          locations.query.category1Id = category1id;\n        } else if (category2id) {\n          locations.query.category2Id = category2id;\n        } else {\n          locations.query.category1Id = category1id;\n        }\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2EA;AACA;AACA;EACAA,eADA;;EAEA;EACAC;IACA;MACAC;IADA;EAGA,CAPA;;EAQAC;IACA,qCADA,CACA;EACA,CAVA;;EAWAC,YACA;MACAC;IADA;EADA,CAXA;EAgBAC;IACA;IACAC;MACA;IACA,CAFA,EAEA,EAFA,CAFA;;IAKAC;MACA;IACA,CAPA;;IAQAC;MACA;MACA;QAAAC;QAAAC;QAAAC;QAAAC;MAAA,IACAC,kBADA;;MAGA;QACA;UACAd,cADA;UAEAe;YAAAC;UAAA;QAFA;;QAIA;UACAC;QACA,CAFA,MAEA;UACAA;QACA,CAFA,MAEA;UACAA;QACA;MACA;IACA;;EA1BA;AAhBA","names":["name","data","currentIndex","mounted","computed","categoryList","methods","enterHandle","leaveHandle","goSearch","categoryname","category1id","category2id","category3id","targetNode","query","categoryName","locations"],"sourceRoot":"src/components/TypeNav","sources":["index.vue"],"sourcesContent":["<template>\n  <!-- 商品分类导航 -->\n  <div class=\"type-nav\">\n    <div class=\"container\">\n      <!--  事件委派 -->\n      <div @mouseleave=\"leaveHandle\" @click=\"goSearch\">\n        <h2 class=\"all\">全部商品分类</h2>\n        <div class=\"sort\">\n          <div class=\"all-sort-list2\">\n            <div\n              class=\"item\"\n              v-for=\"(c1, index) in categoryList\"\n              :key=\"c1.categoryId\"\n            >\n              <h3\n                @mouseenter=\"enterHandle(index)\"\n                :class=\"{ active: currentIndex == index }\"\n              >\n                <a\n                  :data-categoryName=\"c1.categoryName\"\n                  :data-category1Id=\"c1.categoryId\"\n                  >{{ c1.categoryName }}</a\n                >\n              </h3>\n              <div\n                class=\"item-list clearfix\"\n                :style=\"{ display: currentIndex == index ? 'block' : 'none' }\"\n              >\n                <div\n                  class=\"subitem\"\n                  v-for=\"(c2, index) in c1.categoryChild\"\n                  :key=\"c2.categoryId\"\n                >\n                  <dl class=\"fore\">\n                    <dt>\n                      <a\n                        :data-categoryName=\"c2.categoryName\"\n                        :data-category2Id=\"c2.categoryId\"\n                        >{{ c2.categoryName }}</a\n                      >\n                    </dt>\n                    <dd>\n                      <em\n                        v-for=\"(c3, index) in c2.categoryChild\"\n                        :key=\"c3.categoryId\"\n                      >\n                        <a\n                          :data-categoryName=\"c3.categoryName\"\n                          :data-category3Id=\"c3.categoryId\"\n                          >{{ c3.categoryName }}</a\n                        >\n                      </em>\n                    </dd>\n                  </dl>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <nav class=\"nav\">\n        <a href=\"###\">服装城</a>\n        <a href=\"###\">美妆馆</a>\n        <a href=\"###\">尚品汇超市</a>\n        <a href=\"###\">全球购</a>\n        <a href=\"###\">闪购</a>\n        <a href=\"###\">团购</a>\n        <a href=\"###\">有趣</a>\n        <a href=\"###\">秒杀</a>\n      </nav>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapState } from \"vuex\";\nimport throttle from \"lodash/throttle\";\nexport default {\n  name: \"TypeNav\",\n  // 组件挂载完毕，向服务器发送请求\n  data() {\n    return {\n      currentIndex: -1,\n    };\n  },\n  mounted() {\n    this.$store.dispatch(\"categoryList\"); // 派发action\n  },\n  computed: {\n    ...mapState({\n      categoryList: (state) => state.home.categoryList,\n    }),\n  },\n  methods: {\n    // 三级联动节流 throttle\n    enterHandle: throttle(function (index) {\n      this.currentIndex = index;\n    }, 10),\n    leaveHandle() {\n      this.currentIndex = -1;\n    },\n    goSearch(event) {\n      let targetNode = event.target;\n      let { categoryname, category1id, category2id, category3id } =\n        targetNode.dataset;\n\n      if (categoryname) {\n        var locations = {\n          name: \"search\",\n          query: { categoryName: categoryname },\n        };\n        if (category1id) {\n          locations.query.category1Id = category1id\n        } else if (category2id) {\n          locations.query.category2Id = category2id\n        } else {\n          locations.query.category1Id = category1id\n        }\n      }\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"less\">\n.type-nav {\n  border-bottom: 2px solid #e1251b;\n\n  .container {\n    width: 1200px;\n    margin: 0 auto;\n    display: flex;\n    position: relative;\n\n    .all {\n      width: 210px;\n      height: 45px;\n      background-color: #e1251b;\n      line-height: 45px;\n      text-align: center;\n      color: #fff;\n      font-size: 14px;\n      font-weight: bold;\n    }\n\n    .nav {\n      a {\n        height: 45px;\n        margin: 0 22px;\n        line-height: 45px;\n        font-size: 16px;\n        color: #333;\n      }\n    }\n\n    .sort {\n      position: absolute;\n      left: 0;\n      top: 45px;\n      width: 210px;\n      height: 461px;\n      position: absolute;\n      background: #fafafa;\n      z-index: 999;\n\n      .all-sort-list2 {\n        .item {\n          h3 {\n            line-height: 30px;\n            font-size: 14px;\n            font-weight: 400;\n            overflow: hidden;\n            padding: 0 20px;\n            margin: 0;\n\n            a {\n              color: #333;\n            }\n\n            &.active {\n              background: yellowgreen;\n            }\n          }\n\n          .item-list {\n            display: none;\n            position: absolute;\n            width: 734px;\n            min-height: 460px;\n            background: #f7f7f7;\n            left: 210px;\n            border: 1px solid #ddd;\n            top: 0;\n            z-index: 9999 !important;\n\n            .subitem {\n              float: left;\n              width: 650px;\n              padding: 0 4px 0 8px;\n\n              dl {\n                border-top: 1px solid #eee;\n                padding: 6px 0;\n                overflow: hidden;\n                zoom: 1;\n\n                &.fore {\n                  border-top: 0;\n                }\n\n                dt {\n                  float: left;\n                  width: 54px;\n                  line-height: 22px;\n                  text-align: right;\n                  padding: 3px 6px 0 0;\n                  font-weight: 700;\n                }\n\n                dd {\n                  float: left;\n                  width: 415px;\n                  padding: 3px 0 0;\n                  overflow: hidden;\n\n                  em {\n                    float: left;\n                    height: 14px;\n                    line-height: 14px;\n                    padding: 0 8px;\n                    margin-top: 5px;\n                    border-left: 1px solid #ccc;\n                  }\n                }\n              }\n            }\n          }\n\n          &:hover {\n            .item-list {\n              display: block;\n            }\n          }\n        }\n      }\n    }\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}